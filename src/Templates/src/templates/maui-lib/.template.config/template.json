{
    "$schema": "http://json.schemastore.org/template",
    "author": "Microsoft",
    "classifications": [ "MAUI", "Android", "iOS", "macOS", "Mac Catalyst", "WinUI" ],
    "identity": "Microsoft.Maui.MauiLib",
    "groupIdentity": "Microsoft.Maui.Library",
    "name": ".NET MAUI Class Library (Preview)",
    "description": "A project for creating a .NET MAUI class library",
    "shortName": "mauilib",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "MauiLib1",
    "primaryOutputs": [
      { "path": "MauiLib1.csproj" },
      {
        "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
        "path": "Class1.cs"
      }
    ],
    "postActions": [
      {
        "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
        "description": "Opens Class1.cs in the editor.",
        "manualInstructions": [],
        "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EE6",
        "args": {
          "files": "1"
        },
        "continueOnError": true
      }
    ],
    "preferNameDirectory": true,
    "symbols": {
    },
    "defaultName": "MauiLib1"
  }
